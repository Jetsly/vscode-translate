{
  "name": "vscode-translate",
  "displayName": "vscode-translate",
  "description": "Quickly translate text right in your code",
  "version": "0.0.1",
  "publisher": "ddot",
  "icon": "resources/translate.png",
  "bugs": {
    "url": "https://github.com:Jetsly/vscode-translate/issues",
    "email": "cnljli@live.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jetsly/vscode-translate.git"
  },
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "translate",
    "google",
    "google.cn"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:translate.action.translateText"
  ],
  "contributes": {
    "commands": [
      {
        "command": "translate.action.translateText",
        "title": "Translate: Translate selection(s)",
      }
    ],
    "keybindings": [
      {
        "command": "translate.action.translateText",
        "key": "shift+alt+t",
        "mac": "shift+options+t",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "iconv-lite": "^0.4.24",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "urlencode": "^1.1.0"
  }
}
